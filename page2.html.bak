        .page-title {
            text-align: center;
            color: white;
            font-size: 3rem;
            font-family: 'Dancing Script', cursive;
            margin-bottom: 40px;
        }

        .photo-wall {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 20px;
            max-width: 1200px;
            margin: 0 auto;
        }

        .photo-item {
            position: relative;
            border-radius: 15px;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
            transition: transform 0.3s ease;
        }

        .photo-item:hover {
            transform: scale(1.05);
        }

        .photo-item img {
            width: 100%;
            height: 250px;
            object-fit: cover;
        }

        .photo-caption {
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
            background: rgba(0, 0, 0, 0.7);
            color: white;
            padding: 15px;
            font-size: 1rem;
            text-align: center;
        }

        @media (max-width: 768px) {
            .page-title {
                font-size: 2rem;
            }
            
            .photo-wall {
                grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
